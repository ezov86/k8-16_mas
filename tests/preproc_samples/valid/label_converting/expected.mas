# Macroinstructions:
%i i1 {
i1~m1~l~2:
l:
    a @i1~m1~l~2;
i1~m2~l~3:
i1~l:
    b @i1~m2~l~3;
i1~m2~m1~l~0~3:
    a @i1~m2~m1~l~0~3;
i1~m3~l~4:
    c @i1~m3~l~4;
i1~m3~m2~l~1~4:
    b @i1~m3~m2~l~1~4;
i1~m3~m2~m1~l~0~1~4:
    a @i1~m3~m2~m1~l~0~1~4;
}

%i i2 {
i2~m3~l~5:
i2~l:
    c @i2~m3~l~5;
i2~m3~m2~l~1~5:
    b @i2~m3~m2~l~1~5;
i2~m3~m2~m1~l~0~1~5:
    a @i2~m3~m2~m1~l~0~1~5;
}

